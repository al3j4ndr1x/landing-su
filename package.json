{
  "name": "landing-su",
  "jest": {
    "verbose": true,
    "coverageDirectory": "coverage",
    "collectCoverage": false,
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": -10
      }
    },
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "jest-junit": {
    "output": "./test-results/junit/jest-junit.xml"
  },
  "version": "0.1.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot=true",
    "start:staging": "ng serve --aot=true --configuration=staging",
    "build": "ng build",
    "build:staging": "ng build --configuration=staging --aot=true",
    "build:stagelocal": "ng build --env=staging",
    "build:prod": "ng build --prod",
    "lint": "ng lint",
    "cy:verify": "cypress verify",
    "cy:version": "cypress version",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "e2e": "cypress run",
    "cy:chrome": "cypress run --browser chrome",
    "cy:record": "cypress run --record --group all-tests",
    "cy:record:parallel": "cypress run --record --parallel --group all-tests",
    "e2e:ci": "start-server-and-test start http://localhost:4200 cy:record",
    "e2e:ci:staging": "start-server-and-test start:staging http://localhost:4200 cy:record",
    "test": "ng test --runInBand --bail --colors",
    "test:prepush": "ng test --runInBand --bail --colors --coverage",
    "test:watch": "ng test --watch --runInBand --bail --colors",
    "test:cov": "ng test --ci --runInBand --bail --colors --coverage --reporters=default && coveralls < coverage/lcov.info",
    "test:report": "ng test --ci --runInBand --reporters=default --reporters=jest-junit",
    "test:summary": "ng test --ci --runInBand",
    "test:exper": "ng test --ci --runInBand --collectCoverage --bail --colors --reporters=default && coveralls < coverage/lcov.info",
    "test:coveralls": "ng test --ci --runInBand --bail --colors --collectCoverage --reporters=default --coverageReporters=text-lcov | coveralls",
    "pretest:combined": "shx rm -rf ./.nyc_output || shx true",
    "mkdir:reports": "shx mkdir reports || shx true",
    "precopy:reports": "npm run mkdir:reports",
    "test:combined": "npm run test:prepush && npm run cy:run",
    "copy:reports": "shx cp cypress-coverage/coverage-final.json reports/from-cypress.json && shx cp jest-coverage/coverage-final.json reports/from-jest.json",
    "precombine:reports": "npm run copy:reports && shx mkdir ./.nyc_output || shx true",
    "combine:reports": "nyc merge reports coverage.json && shx mv coverage.json ./.nyc_output/out.json",
    "prereport:combined": "npm run combine:reports",
    "report:combined": "nyc report --reporter --report-dir coverage",
    "posttest:combined": "npm run report:combined",
    "cy:cov": "start-server-and-test start http://localhost:4200 cy:run",
    "cov:report": "nyc report --reporter=text-summary --report-dir coverage",
    "postinstall": "ngcc"
  },
  "author": "al3j4ndr1x",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.3",
    "@angular/cdk": "^9.2.1",
    "@angular/common": "^9.1.3",
    "@angular/compiler": "^9.1.3",
    "@angular/core": "^9.1.3",
    "@angular/fire": "^6.1.4",
    "@angular/forms": "^9.1.3",
    "@angular/localize": "^9.1.3",
    "@angular/platform-browser": "^9.1.3",
    "@angular/platform-browser-dynamic": "^9.1.3",
    "@angular/router": "^9.1.3",
    "@angular/service-worker": "^9.1.3",
    "@nebular/auth": "^5.0.0",
    "@nebular/date-fns": "^5.0.0",
    "@nebular/eva-icons": "^5.0.0",
    "@nebular/theme": "^5.0.0",
    "date-fns": "^2.16.1",
    "eva-icons": "^1.1.3",
    "firebase": "^8.1.2",
    "lodash": "^4.17.20",
    "rxjs": "^6.5.5",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^9.0.1",
    "@angular-devkit/build-angular": "~0.901.3",
    "@angular-devkit/schematics": "^11.0.4",
    "@angular/cli": "^9.1.12",
    "@angular/compiler-cli": "^9.1.3",
    "@angular/language-service": "^9.1.3",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@cypress/code-coverage": "^3.5.2",
    "@cypress/webpack-preprocessor": "^5.1.2",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chance": "^1.0.10",
    "@types/dotenv": "^8.2.0",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "babel-plugin-istanbul": "^6.0.0",
    "chance": "^1.1.7",
    "codelyzer": "^6.0.1",
    "coveralls": "^3.1.0",
    "cypress": "^4.4.1",
    "dotenv": "^8.2.0",
    "firebase-tools": "^8.18.1",
    "husky": "^4.3.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "istanbul-lib-coverage": "^3.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "ngx-build-plus": "^9.0.6",
    "nyc": "^15.1.0",
    "protractor": "~5.4.4",
    "shx": "^0.3.3",
    "source-map-support": "^0.5.19",
    "start-server-and-test": "^1.11.6",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.1",
    "ts-node": "^8.9.0",
    "tslint": "~6.1.1",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm run test:prepush"
    }
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "cache": true,
    "report-dir": "cypress-coverage"
  }
}
